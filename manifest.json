{
    "manifest_version": 3,
    "name": "Study Music Player",
    "version": "1.0",
    "description": "Play binaural beats and study music from your browser popup",
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/*",
          "https://www.twitch.tv/*",
          "https://www.ebay.com/*"
        ],
        "js": ["jquery.min.js", "./main.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "focus mode"
    },
    "permissions": ["storage", "activeTab"]
  }
  